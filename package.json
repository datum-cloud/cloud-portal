{
  "name": "cloud-portal",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "react-router build",
    "dev": "cross-env NODE_ENV=development tsx watch ./server.ts",
    "start": "cross-env NODE_ENV=production bun run ./docker-start.js",
    "preview": "cross-env NODE_ENV=production tsx ./server.ts",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "typecheck": "react-router typegen && tsc",
    "format": "prettier --write .",
    "prepare": "husky",
    "clean": "rm -rf node_modules && rm -rf .react-router && rm -rf build",
    "openapi-ts": "openapi-ts",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "test:e2e": "start-server-and-test dev http://localhost:3000 'cypress run'",
    "test:e2e:prod": "start-server-and-test 'npm run preview' http://localhost:3000 'cypress run --config-file cypress.config.ts --quiet'",
    "test:e2e:debug": "start-server-and-test 'npm run preview' http://localhost:3000 'cypress open'",
    "test:component": "cypress run --component --config-file cypress.config.ts --quiet",
    "test:component:open": "cypress open --component"
  },
